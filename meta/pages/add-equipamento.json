{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Equipamento!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ProjetoNew.createEquipamento",
                        "parameters": [
                          {
                            "name": "Equipamento",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "property": "no_regis_produto",
                      "required": true,
                      "title": "No Regis Produto",
                      "type": "integer"
                    },
                    {
                      "property": "nome_equi",
                      "required": true,
                      "title": "Nome Equi",
                      "type": "string",
                      "maxLength": 100
                    },
                    {
                      "data": "${getRecursosResult}",
                      "property": "index_Recurso",
                      "required": true,
                      "textProperty": "obs",
                      "title": "Recurso",
                      "type": "lookup",
                      "valueProperty": "index_Recurso",
                      "placeholder": "Choose Recurso"
                    },
                    {
                      "property": "garantia",
                      "required": false,
                      "title": "Garantia",
                      "type": "date"
                    },
                    {
                      "property": "categoria",
                      "required": false,
                      "title": "Categoria",
                      "type": "string",
                      "maxLength": 100
                    },
                    {
                      "property": "Equi_obs",
                      "required": false,
                      "title": "Equi Obs",
                      "type": "string",
                      "maxLength": 100
                    }
                  ],
                  "name": "form0",
                  "type": "form"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ProjetoNew.getRecursos",
        "parameters": [],
        "then": [
          {
            "name": "getRecursosResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Add Equipamento",
  "layout": "Main"
}