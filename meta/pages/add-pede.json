{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Pede!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ProjetoNew.createPede",
                        "parameters": [
                          {
                            "name": "Pede",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "data": "${getPessoasResult}",
                      "property": "id_Pessoa",
                      "required": true,
                      "textProperty": "email_pessoa",
                      "title": "Pessoa",
                      "type": "lookup",
                      "valueProperty": "id_Pessoa",
                      "placeholder": "Choose Pessoa"
                    },
                    {
                      "data": "${getServicosResult}",
                      "property": "index_servicos",
                      "required": true,
                      "textProperty": "designa_pt_servicos",
                      "title": "Servico",
                      "type": "lookup",
                      "valueProperty": "index_servicos",
                      "placeholder": "Choose Servico"
                    }
                  ],
                  "name": "form0",
                  "type": "form"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ProjetoNew.getPessoas",
        "parameters": [],
        "then": [
          {
            "name": "getPessoasResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "ProjetoNew.getServicos",
        "parameters": [],
        "then": [
          {
            "name": "getServicosResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Add Pede",
  "layout": "Main"
}