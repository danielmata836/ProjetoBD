{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "${gt}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Gt",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ProjetoNew.updateGt",
                        "parameters": [
                          {
                            "name": "id_GT",
                            "value": "${parameters.id_GT}"
                          },
                          {
                            "name": "Gt",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "property": "sigla",
                      "required": true,
                      "title": "Sigla",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "property": "id_GT",
                      "required": true,
                      "title": "Id Gt",
                      "type": "integer",
                      "requiredText": "is required",
                      "minText": "cannot be less than",
                      "maxText": "cannot be more than"
                    },
                    {
                      "property": "nome_GT",
                      "required": true,
                      "title": "Nome Gt",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    }
                  ],
                  "name": "form0",
                  "type": "form",
                  "visible": "gt"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ProjetoNew.getGtByid_GT",
        "parameters": [
          {
            "name": "id_GT",
            "value": "${parameters.id_GT}"
          }
        ],
        "then": [
          {
            "name": "gt",
            "type": "set",
            "value": "${result}"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Gt",
  "layout": "Main",
  "access": [
    "Admin"
  ]
}