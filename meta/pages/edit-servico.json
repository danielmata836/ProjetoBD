{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "${servico}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Servico",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ProjetoNew.updateServico",
                        "parameters": [
                          {
                            "name": "index_servicos",
                            "value": "${parameters.index_servicos}"
                          },
                          {
                            "name": "Servico",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "property": "designa_pt_servicos",
                      "required": true,
                      "title": "Designa Pt Servicos",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "property": "designa_en_servicos",
                      "required": true,
                      "title": "Designa En Servicos",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "property": "inst_dispo_servicos",
                      "required": true,
                      "title": "Inst Dispo Servicos",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "property": "index_servicos",
                      "required": true,
                      "title": "Index Servicos",
                      "type": "integer",
                      "requiredText": "is required",
                      "minText": "cannot be less than",
                      "maxText": "cannot be more than"
                    },
                    {
                      "property": "obs_servicos",
                      "required": true,
                      "title": "Obs Servicos",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "property": "custo_servicos",
                      "required": true,
                      "title": "Custo Servicos",
                      "type": "number",
                      "requiredText": "is required",
                      "minText": "cannot be less than",
                      "maxText": "cannot be more than"
                    },
                    {
                      "property": "desc_servicos",
                      "required": true,
                      "title": "Desc Servicos",
                      "type": "string",
                      "maxLength": 100,
                      "requiredText": "is required"
                    },
                    {
                      "data": "${getGtsResult}",
                      "property": "id_GT",
                      "required": true,
                      "textProperty": "sigla",
                      "title": "Gt",
                      "type": "lookup",
                      "valueProperty": "id_GT",
                      "placeholder": "Choose Gt",
                      "requiredText": "is required"
                    },
                    {
                      "property": "data_inicio",
                      "required": false,
                      "title": "Data Inicio",
                      "type": "date",
                      "requiredText": "is required"
                    },
                    {
                      "property": "data_fim",
                      "required": false,
                      "title": "Data Fim",
                      "type": "date",
                      "requiredText": "is required"
                    }
                  ],
                  "name": "form0",
                  "type": "form",
                  "visible": "servico"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ProjetoNew.getServicoByindex_servicos",
        "parameters": [
          {
            "name": "index_servicos",
            "value": "${parameters.index_servicos}"
          }
        ],
        "then": [
          {
            "name": "servico",
            "type": "set",
            "value": "${result}"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "ProjetoNew.getGts",
        "parameters": [],
        "then": [
          {
            "name": "getGtsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Servico",
  "layout": "Main",
  "access": [
    "Staff"
  ]
}