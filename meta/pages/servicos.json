{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Servicos"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "name": "grid0",
                  "allowDelete": true,
                  "confirmDelete": true,
                  "allowAdd": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "count": "${getServicosCount}",
                  "data": "${getServicosResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "ProjetoNew.getServicos",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Gt"
                          }
                        ],
                        "then": [
                          {
                            "name": "getServicosResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getServicosCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load Servicos",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Servico",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ProjetoNew.deleteServico",
                        "parameters": [
                          {
                            "name": "index_servicos",
                            "value": "${event.index_servicos}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Servico deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "add": [
                      {
                        "path": "add-servico",
                        "type": "popup"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "index_servicos",
                            "value": "${event.index_servicos}"
                          }
                        ],
                        "path": "edit-servico",
                        "type": "popup"
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "designa_pt_servicos",
                      "title": "Designa Pt Servicos",
                      "type": "string"
                    },
                    {
                      "property": "designa_en_servicos",
                      "title": "Designa En Servicos",
                      "type": "string"
                    },
                    {
                      "property": "inst_dispo_servicos",
                      "title": "Inst Dispo Servicos",
                      "type": "string"
                    },
                    {
                      "property": "index_servicos",
                      "title": "Index Servicos",
                      "type": "integer"
                    },
                    {
                      "property": "obs_servicos",
                      "title": "Obs Servicos",
                      "type": "string"
                    },
                    {
                      "property": "custo_servicos",
                      "title": "Custo Servicos",
                      "type": "number",
                      "format": "double"
                    },
                    {
                      "property": "desc_servicos",
                      "title": "Desc Servicos",
                      "type": "string"
                    },
                    {
                      "property": "id_GT",
                      "sortProperty": "Gt.sigla",
                      "filterProperty": "Gt.sigla",
                      "template": "${data.Gt?.sigla}",
                      "title": "Gt",
                      "type": "string"
                    },
                    {
                      "property": "data_inicio",
                      "formatString": "medium",
                      "title": "Data Inicio",
                      "type": "string",
                      "format": "date-time"
                    },
                    {
                      "property": "data_fim",
                      "formatString": "medium",
                      "title": "Data Fim",
                      "type": "string",
                      "format": "date-time"
                    }
                  ],
                  "type": "grid"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "type": "execute",
        "code": "this.grid0.load();"
      }
    ]
  },
  "name": "Servicos",
  "layout": "Main"
}