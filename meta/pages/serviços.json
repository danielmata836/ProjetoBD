                    },
                    {
                      "property": "obs_servicos",
                      "title": "Obs Servicos",
                      "type": "string"
                    },
                    {
                      "property": "custo_servicos",
                      "title": "Custo Servicos",
                      "type": "number",
                      "format": "double"
                    },
                    {
                      "property": "desc_servicos",
                      "title": "Desc Servicos",
                      "type": "string"
                    },
                    {
                      "property": "id_GT",
